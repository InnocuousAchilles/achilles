<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sp.qrcode" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>achilles</name>
    <description>
        A scouting application to be used in steamworks.
    </description>
    <author email="achilles@jcharante.com" href="https://jcharante.com">
        JCharante
    </author>
    <content src="templates/home/index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <icon src="www/static/internal/img/favicon/android-icon-192x192.png" />
    <engine name="android" spec="~4.0.2" />
    <plugin name="cordova-plugin-qrscanner" spec="~2.5.0" />
</widget>
